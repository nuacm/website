<%= form_for @member do |f| %>
  <% @member.errors.full_messages.each do |error_message| %>
    <%= error_message %><br>
  <% end %>
  <br>


  <%= f.label :full_name, 'Full Name' %>:
  <%= f.text_field :full_name %><br />
  <%= f.label :email, 'Email' %>:
  <%= f.text_field :email %><br />
  <%= f.submit %>
<% end %>